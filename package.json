{
  "name": "vmarta",
  "version": "2.0.0-dev",
  "license": "See license in LICENSE",
  "private": true,
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "@remix-run/express": "^1.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "pwa-kit-build": "^2.0.0-dev",
    "pwa-kit-runtime": "^2.0.0-dev",
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "mrt test",
    "format": "mrt format '\"**/*.{js,jsx}\"'",
    "lint": "mrt lint '\"**/*.{js,jsx}\"'",
    "lint:fix": "npm run lint -- --fix",
    "start": "mrt start",
    "build": "mrt build && npm run remix:build",
    "push": "mrt push",
    "save-credentials": "mrt save-credentials",
    "remix:build": "remix build",
    "remix:dev": "remix watch",
    "postinstall": "remix setup node",
    "remix:start": "cross-env NODE_ENV=production node server/index.js",
    "remix:start-dev": "cross-env NODE_ENV=development node server/index.js"
  },
  "mobify": {
    "ssrEnabled": true,
    "ssrParameters": {
      "ssrFunctionNodeVersion": "14.x"
    },
    "ssrOnly": [
      "ssr.js",
      "node_modules/**/*.*"
    ],
    "ssrShared": [
      "intentionally-does-not-exist"
    ]
  }
}
